<p style="color: green"><%= notice %></p>

<% content_for :title, "Characters" %>

<h1>Characters</h1>

<div id="characters">
  <% @characters.each do |character| %>
    <%= render character %>
    <%= link_to "Show this character", character %>
  <% end %>
</div>
<%= link_to "New character", new_character_path %>

